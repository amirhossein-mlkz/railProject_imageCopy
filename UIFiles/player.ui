/* Main window background */
QWidget {
    background-color: #2E2E2E; /* Dark gray, resembling a military interface */
}

/* Video widget border and background */
QVideoWidget {
    background-color: #000000; /* Black background for the video */
    border: 2px solid #4B5320; /* Army green border */
    border-radius: 5px;
}

/* Play and Pause buttons */
QPushButton {
    background-color: #4B5320; /* Army green button */
    color: #FFFFFF; /* White text */
    font-weight: bold;
    padding: 10px;
    border-radius: 5px;
    border: 2px solid #808000; /* Olive border */
}

QPushButton:hover {
    background-color: #556B2F; /* Dark olive green on hover */
}

QPushButton:pressed {
    background-color: #6B8E23; /* Light olive green when pressed */
}

/* Video progress slider */
QSlider::groove:horizontal {
    border: 1px solid #4B5320; /* Army green */
    height: 8px;
    background-color: #3B3B3B; /* Dark gray groove */
    border-radius: 4px;
}

QSlider::handle:horizontal {
    background-color: #808000; /* Olive handle */
    border: 2px solid #4B5320;
    width: 15px;
    height: 15px;
    border-radius: 7px;
}

QSlider::handle:horizontal:hover {
    background-color: #556B2F; /* Dark olive green on hover */
}

QSlider::sub-page:horizontal {
    background-color: #556B2F; /* Dark olive green for the part of the slider that indicates progress */
    border-radius: 4px;
}

/* Time label */
QLabel {
    color: #C0C0C0; /* Light gray text for the time label */
    font-family: &quot;Arial&quot;;
    font-size: 14px;
    font-weight: bold;
}

